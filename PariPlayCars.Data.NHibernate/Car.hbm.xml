				   assembly="NHibernate"
				   namespace="NHibernate">
	<class name="Car">
		<id name="Id">
			<generator class="native"/>
		</id>
		<property name="Brand"/>
		<property name="Model"/>
		<property name="Year"/>
	</class>
</hibernate-mapping>